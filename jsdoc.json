{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include":  [
          "src",
          "dist"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "((^|\\/|\\\\)_|\\.test\\.|\\.gitignore)"
    },
    "markdown": {
      "idInHeadings": true
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
          "staticFiles": {
            "include": ["docs/Static"]
          },
          "includeDate": false
        }
    },
    "opts": {
      "destination": "docs/HTML",
      "tutorials": "docs/Source/tutorials",
      "readme": "docs/Source/README.md"
    }
}
